{
  "name": "@emdzej/wds-importer",
  "version": "0.1.0",
  "description": "BMW WDS data import and transformation tool",
  "type": "module",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -b",
    "typecheck": "tsc --noEmit",
    "dev": "tsx src/index.ts",
    "import": "tsx src/index.ts import",
    "import:dry": "tsx src/index.ts import --dry-run",
    "validate": "tsx src/index.ts validate",
    "stats": "tsx src/index.ts stats",
    "test": "vitest run",
    "lint": "echo 'no lint'",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "keywords": [
    "wds",
    "bmw",
    "wiring",
    "diagrams",
    "import"
  ],
  "author": "emdzej",
  "license": "UNLICENSED",
  "dependencies": {
    "@emdzej/wds-core": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "fast-xml-parser": "^4.3.0",
    "globby": "^14.0.0",
    "jsdom": "^24.1.3",
    "p-queue": "^8.0.0",
    "turndown": "^7.1.0"
  },
  "devDependencies": {
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
